extends layout

block content
    .well
        button#serverButton(type='button' action='' class='btn btn-default' ) server
        button#clientButton(type='button' class='btn btn-default' ) klijent
        
    script.
        $('#serverButton').click(function(evt){
            window.location = 'server';
            return;
        });

        $('#clientButton').click(function(evt){
            window.location = 'home';
            return;
        });